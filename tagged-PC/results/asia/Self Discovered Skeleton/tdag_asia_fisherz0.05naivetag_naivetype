digraph G {
	asia [fillcolor="#8dd3c7" style=filled]
	tub [fillcolor="#8dd3c7" style=filled]
	smoke [fillcolor="#ffffb3" style=filled]
	lung [fillcolor="#ffffb3" style=filled]
	bronc [fillcolor="#bebada" style=filled]
	either [fillcolor="#bebada" style=filled]
	xray [fillcolor="#fb8072" style=filled]
	dysp [fillcolor="#fb8072" style=filled]
	asia -> tub [label="" color=green]
	smoke -> lung [label="" color=green]
	smoke -> bronc [label="" color=green]
	lung -> smoke [label="" color=green]
	lung -> either [label="" color=green]
	bronc -> smoke [label="" color=green]
	bronc -> dysp [label="" color=green]
	either -> tub [label="" color=green]
	either -> lung [label="" color=green]
	either -> xray [label="" color=green]
	either -> dysp [label="" color=green]
}
